/* css reset
----------------------------------------------------------*/
* { margin: 0; padding: 0; font-family: Arial; }
html { -webkit-text-size-adjust: none; }
html, body { height: 100%; }
body, ul, ol, p, h1, h2, h3, h4, h5, h6, dl, dt, dd, form, textarea, input { margin: 0; padding: 0; }
body { font: normal 12px/1.5 Verdana,Arial,sans-serif, "Times New Roman",宋体; color: #000; background: #fff; }
    body div { text-align: left; }
input, select { font-size: 12px; vertical-align: middle; }
select { height: 28px; }
table { margin: 0 auto; width: 100%; border-collapse: collapse; }
a { text-decoration: none; color: #333; }
    a:hover { text-decoration: underline; }
    a img { border: none; }
label { cursor: pointer; }
textarea, input { word-wrap: break-word; overflow: hidden; }
li { list-style-type: none; }
.pointer { cursor: pointer; }
.NoBorder { border: none; }
.t-l { text-align: left; }
.t-r { text-align: right; }
.t-c { text-align: center; }
.Break { word-wrap: break-word; word-break: break-all; }
.Nobreak { word-break: break-all; word-wrap: normal; }
/* HEADINGS   
----------------------------------------------------------*/
#Header { width: 100%; text-decoration: none; }
#topHR { color: #008a5e; background: #008a5e; height: 8px; width: 100%; }
#logoDiv { height: 48px; display: inline; /*margin: -12px auto;*/ }
#appName { height: 48px; vertical-align: bottom; line-height: 48px; font-size: 32px; font-weight: bold; /*float: right; margin-right: 15px;*/ }
/* Menu   
----------------------------------------------------------*/
#nav { font-size: 18px; border: 1px solid #121314; border-top: 1px solid #2b2e30; overflow: hidden; background: #005288; height: 30px; padding: 0 10px; }
    #nav .userInfo { color: #FFF; font-weight: bold; float: right; height: 32px; line-height: 32px; /*margin-right:300px*/ }
    #nav ul { margin: 0; padding: 0; list-style: none; }
        #nav ul li { float: left; height: 30px; line-height: 30px; }
            #nav ul li a { /*background: none repeat scroll 0 0 #005288; border-left: 1px solid #005288; border-right: 1px solid #005288;*/ color: #FFFFFF; float: left; padding: 0 20px; text-decoration: none; }
                #nav ul li a:hover, #nav ul li:hover > a { background: #87CEEB; color: Black; }
            #nav li ul a:hover, #nav ul li li:hover > a { background: #87CEEB; color: Black; border-bottom: 1px solid #3C4042; border-top: 1px solid #3C4042; }
    #nav li ul { background: #005288; -moz-border-radius: 0 0 10px 10px; -webkit-border-radius: 0 0 10px 10px; left: -999em; position: absolute; width: 160px; z-index: 9999; -moz-box-shadow: 0 0 15px rgba(0, 0, 0, 0.4) inset; -webkit-box-shadow: 0 0 15px rgba(0, 0, 0, 0.4) inset; border: 1px solid rgba(0, 0, 0, 0.5); }
    #nav li:hover ul { left: auto; }
    #nav li ul a { background: none; border: 0 none; margin-right: 0; width: 120px; -moz-box-shadow: none; -webkit-box-shadow: none; border-bottom: 1px solid transparent; border-top: 1px solid transparent; }
    #nav li li ul { margin: -1px 0 0 160px; -webkit-border-radius: 0 10px 10px 10px; -moz-border-radius: 0 10px 10px 10px; visibility: hidden; }
    #nav li li:hover ul { visibility: visible; }
    #nav ul ul li:last-child > a { -moz-border-radius: 0 0 10px 10px; -webkit-border-radius: 0 0 10px 10px; }
    #nav ul ul ul li:first-child > a { -moz-border-radius: 0 10px 0 0; -webkit-border-radius: 0 10px 0 0; }

.active {
    background: #87CEEB;
    color: Black;
}
/* Body   
----------------------------------------------------------*/
#ui-datepicker-div { display: none; }
.mainContainer { margin: 0; padding: 0; text-align: center; width: 100%; overflow: hidden; }
.dataTable { /*font-family:Verdana, Arial, Helvetica, sans-serif; */ border: 1px solid #999999; width: 100%; font-size: 9pt; text-align: center; vertical-align: middle; margin: 0; border-collapse: collapse; table-layout: fixed; word-break: break-all; white-space: nowrap; }
    .dataTable td { border: 1px solid #999999; padding: 0 5px; color: #000000; margin: 0; overflow: hidden; text-overflow: ellipsis; vertical-align: top; }
    .dataTable tbody tr { height: 27px; line-height: 27px; }
    .dataTable tbody a { color: Blue; }
    .dataTable tbody tr:nth-child(even) { background-color: #EFF3FB; }
.Histable td { padding: 0 5px; }
.dataTable th { border: 1px solid #999999; padding: 0 5px 5px; font-weight: bold; text-align: center; color: #FFFFFF; vertical-align: middle; margin: 0 2px 0 2px; position: relative; background-color: #507CD1; }
.SubmitStyle { /*width: 80px;*/ font-family: Consolas; height: 35px !important; line-height: 35px; text-align: center; padding: 0 10px; margin: 10px 5px; }
.formtable { width: 860px; font-size: 14px; }
    .formtable tr { line-height: 20px; }
    .formtable .td1 { text-align: right; padding: 5px 10px; width: 30%; }
    .formtable .td2 { text-align: left; padding: 5px 10px; width: 70%; }
    .formtable td span { font-style: italic; padding-right: 8px; vertical-align: middle; }
label.error { padding-left: 16px; padding-bottom: 2px; font-weight: bold; color: #EA5200; }
input.error, textarea.error { border: 1px solid red; }
label.checked { background: url("../images/checked.gif") no-repeat 0px 0px; }
.formtable textarea { height: 60px; width: 360px; padding: 3px; }
.formtable input[type=text] { padding: 3px; width: 360px; }
.formtable select { width: 370px; padding: 3px; }
.submitdelete { width: 16px; height: 16px; background: url(../images/delete.png); cursor: pointer; border: none; }
.submitadd { width: 16px; height: 16px; background: url(../images/add.png); cursor: pointer; border: none; }
.LocationTag { font-size: 16px; margin: 10px; }
.clear { clear: both; }
.autoBody { height: auto; overflow: visible; width: 372px; padding: 5px 0px; }
.ep-gl-guest { padding-bottom: 2px; position: relative; font-family: Arial,sans-serif; font-size: 12px; width: 370px; height: 20px; top: 0px; left: 0px; }
.a1 { background: url("../images/combined_v39_vr.png") -199px -52px; display: block; position: absolute; right: 0; top: 3px; height: 10px; margin: 3px 0; width: 10px; }
.ep-gc-chip-text { width: 166px; height: 14px; display: inline; margin: 3px 2px 0 1px; font-size: 12px; font-family: Arial; margin-left: 10px; }
